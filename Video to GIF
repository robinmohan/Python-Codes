from moviepy.editor import *

clip = (VideoFileClip("C:/Users/robin.a.gupta/Desktop/Media1.mp4")
        .subclip(0,8)                 #subclip,  #.subclip((1,22.65),(1,23.2))
        .fx( vfx.speedx, 1.75)        #speed of video
        .resize(0.40))
clip.write_gif("use_your_head.gif")  #name of the gif
